<script>
  
    // 右边栏 购物车 在线客服
    $(".contactusdiyou").hover(function () {
        $(".hoverimg").attr("src", "images/hoverbtnbg1.gif");
        $('.diyoumask').fadeIn();
        $('.contactusdiyou').animate({ right: '0' }, 300);
    }, function () {
        $(".hoverimg").attr("src", "images/hoverbtnbg.gif");
        $('.contactusdiyou').animate({ right: '-230px' }, 300, function () { });
        $('.diyoumask').fadeOut();
    });

    // 登录界面交互
    $("#btn").click(function () {
        console.log(1)
        let username = $("#username").val()
        let passwd = $("#password").val()
        $.post("http://192.168.7.221:3000/users/login", { "username": username, "passwd": passwd }, function (result) {
            console.log(result)
            if (result.status == "0") {
                // 返回原页面
                alert('登录成功')
                location = document.referrer
            } else {
                alert('登录失败 送卿一首情诗 尘世三千  吾爱有三 日月与卿 日为朝 月为暮 卿为朝朝暮暮')
            }
        })
    })




    $.ajax({
        url:'http://192.168.7.221:3000/MEC_tijian/taocan',
        type:'get',
        dataType:'jsonp',
        success:function(res){
            var result = res.result
            console.log(result)
            
           

            

            // var $table = $('.MEC-pet-content-hss').children(4).children(3).html()
            $('.MEC-pet-content-taocan-nav').children('li').click(function(){
                $(this).siblings().removeClass('active')
                $(this).addClass('active')
                // var $class = $(this).attr('class')
                // console.log($('.MEC-pet-content-hss').children(4).children(3).children('table'))   
            })

            // var otable =$('table')
            //     var otr0 = $('<tr>')
            //     var oth1 = $('<th class="text-center">体检套餐</th>')
            //     var oth2 = $('<th >套餐原价</th>')
            //     var oth3 = $('<th ><dd>中康优惠价</dd></th>')
            //     var oth4 = $('<th >操作</th>')

            //     otr0.append(oth1)
            //     otr0.append(oth2)
            //     otr0.append(oth3)
            //     otr0.append(oth4)
            //     otable.append(otr0)


            if($('.active').eq(6).text().trim() == '销量排行'){
                for(let i = 0;i < result.length;i++){
                    var otable =$('table')
                    var otr = $('<tr>')
                    var otd1 = $('<td>')
                    var otd2 = $('<td>')
                    var otd3 = $('<td>')
                    var otd4 = $('<td>')
                    var ospan = $('<span>')
                    var odd = $('<dd>')
                    var oa = $('<a>')

                    ospan.text(result[i].name)
                    otd2.text('￥' + result[i].number1 + '.00' )
                    odd.text('￥' + result[i].number2 + '.00')
                    oa.text('查看详情')

                    otd1.append(ospan)
                    otd3.append(odd)
                    otd4.append(oa)
                    otr.append(otd1)
                    otr.append(otd2)
                    otr.append(otd3)
                    otr.append(otd4)
                    otable.append(otr)
                }
            }else{
                for(let i = result.length;i >= 0;i--){
                    var otable =$('table')
                    var otr = $('<tr>')
                    var otd1 = $('<td>')
                    var otd2 = $('<td>')
                    var otd3 = $('<td>')
                    var otd4 = $('<td>')
                    var ospan = $('<span>')
                    var odd = $('<dd>')
                    var oa = $('<a>')

                    ospan.text(result[i].name)
                    otd2.text('￥' + result[i].number1 + '.00' )
                    odd.text('￥' + result[i].number2 + '.00')
                    oa.text('查看详情')

                    otd1.append(ospan)
                    otd3.append(odd)
                    otd4.append(oa)
                    otr.append(otd1)
                    otr.append(otd2)
                    otr.append(otd3)
                    otr.append(otd4)
                    otable.append(otr)
                }
            }

        }
    })
